<?xml version='1.0' encoding='utf-8'?>
<widget id="lhidalgo42.ikigaifoundation.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Ikigai</name>
    <description>
        The Ikigai Foundation
    </description>
    <author email="lhidalgosep@gmail.com" href="http://facebook.com/leoohidalgo"> Leonardo Hidalgo </author>
    <content src="http://app.glocalq.com/login" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
